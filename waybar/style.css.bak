* {
  font-size: 16px;
  font-weight: 600;
  font-family: "JetBrains Mono", "FontAwesome";
  color: #ffffff;
}

#waybar {
  background: #161616;
  border-radius: 0px;
}

.module {
  margin: 4px 20px;
  padding: 1px 15px;
  background: #393939;
  border-radius: 15px;
}

#workspaces {
  padding: 1px 15px;
}

#workspaces button.active {
  border-radius: 30px;
  background-color: #525252;
}
